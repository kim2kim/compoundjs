<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.ebays() %>">ebays</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">New ebay</a></li>
</ul>

<div class="page-header">
    <h1>New ebay</h1>
</div>

<% var form = formFor(ebay, {action: pathTo.ebays(), method: 'POST', id: "ebay-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Create ebay', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.ebays(), {class: 'btn'}) %>
</div>
<%- form.end() %>
