<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.ebays %>">ebays</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of ebay</a></li>
</ul>

<div class="page-header">
    <h1>Details of ebay</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>ebay_id</th><td><%= ebay.ebay_id %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_ebay(ebay), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.ebay(ebay), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.ebays) %>
</div>
