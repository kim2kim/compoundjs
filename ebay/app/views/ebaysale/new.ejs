<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.ebaysale() %>">ebaysales</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">New ebaysale</a></li>
</ul>

<div class="page-header">
    <h1>New ebaysale</h1>
</div>

<% var form = formFor(ebaysale, {action: pathTo.ebaysale(), method: 'POST', id: "ebaysale-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Create ebaysale', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.ebaysale(), {class: 'btn'}) %>
</div>
<%- form.end() %>
